alias web='cd /home/workspace/medusa/medusaserver && sudo python manage.py runserver 0.0.0.0:8000'
alias celery_worker='cd /home/workspace/medusa/medusaserver && python manage.py celery worker --loglevel=info'
alias es='cd /home/vagrant/elasticsearch-1.7.1/bin && ./elasticsearch'
alias logstash='cd /home/vagrant/logstash-2.2.2 && ./bin/logstash -f ./conf/logstash_agent.conf'
alias kibana='cd /home/vagrant/kibana-4.1.5-linux-x64/bin && ./kibana'
alias uwsgi='sudo /usr/local/bin/uwsgi --ini /etc/uwsgi/uwsgi_common.ini:mysite'
alias supervisor='sudo supervisord -c /etc/supervisor/supervisord.conf'
alias mezzanine='cd /home/workspace/medusa_mezzanine && sudo python manage.py runserver 0.0.0.0:8000'
alias supervisor='sudo /usr/bin/python /usr/local/bin/supervisord -c /etc/supervisor/supervisord.conf'
alias sentry_web='cd /home/workspace/sentry && source bin/activate && sentry run web'
alias sentry_worker='cd /home/workspace/sentry && source bin/activate && sentry run worker'
alias sentry_cron='cd /home/workspace/sentry && source bin/activate && sentry run cron'
